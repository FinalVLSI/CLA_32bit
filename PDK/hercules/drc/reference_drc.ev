BOOLEAN alln25_srcdrn AND CONTS 				TEMP = n25diff_cont
BOOLEAN allna25_srcdrn AND CONTS 				TEMP = na25diff_cont
BOOLEAN allp25_srcdrn AND CONTS 				TEMP = p25diff_cont
BOOLEAN alln18_srcdrn AND CONTS 				TEMP = n18diff_cont
