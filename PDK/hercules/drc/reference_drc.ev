       	COMMENT = "2.2.1/2.8.1/2.13/2.14: Minimum Width for RPPOLYS 0.12;Maximum Width (of RPPOLYS ) <= 100; Minimum length >= 0.6;Maximum length <= 1000"
       	EQUATIONS {
             IF(EV_WMIN < 0.12)  EV_ERROR_WMIN=EV_WMIN;
